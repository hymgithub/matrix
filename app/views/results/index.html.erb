<h1>Listing results</h1>

<table>
  <tr>
    <th>Matrix params</th>
    <th>Value</th>
    <th>Is chose</th>
    <th>Sort</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @results.each do |result| %>
  <tr>
    <td><%= result.matrix_params_id %></td>
    <td><%= result.value_id %></td>
    <td><%= result.is_chose %></td>
    <td><%= result.sort_id %></td>
    <td><%= link_to 'Show', result %></td>
    <td><%= link_to 'Edit', edit_result_path(result) %></td>
    <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

