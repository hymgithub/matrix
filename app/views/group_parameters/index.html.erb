<h1>Listing group_parameters</h1>

<table>
  <tr>
    <th>Group</th>
    <th>Parameter</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @group_parameters.each do |group_parameter| %>
  <tr>
    <td><%= group_parameter.group_id %></td>
    <td><%= group_parameter.parameter_id %></td>
    <td><%= group_parameter.status %></td>
    <td><%= link_to 'Show', group_parameter %></td>
    <td><%= link_to 'Edit', edit_group_parameter_path(group_parameter) %></td>
    <td><%= link_to 'Destroy', group_parameter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

